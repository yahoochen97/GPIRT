#!/bin/bash

mkdir -p data
mkdir -p figures
mkdir -p results

MAXSEED=1
declare -a Cs=(5)
declare -a ns=(100)
declare -a ms=(10)

if [ $1 = "run" ]
then
    for SEED in $(seq 1 1 $MAXSEED); do
        for C in ${Cs[@]}; do
            for n in ${ns[@]}; do
                for m in ${ms[@]}; do
                    bsub -G SEAS-Lab-Garnett -o "test.log" -J "C${C}n${n}m${m}_${SEED}" "./onerun $SEED $C $n $m"
                done
            done
        done
    done
fi
